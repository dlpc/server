{
  "name": "tourze/server",
  "description": "途者PHP框架-集成一些server相关的东西",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "tourze",
      "email": "dev@tourze.com"
    }
  ],
  "require": {
    "tourze/base": "^1.0",
    "workerman/workerman": "^3.1",
    "league/climate": "^3.2"
  },
  "require-dev": {
